@{
    ViewData["Title"] = "Apagar Contatos";
}


<div class="text-center">

    <h3>Realmente deseja apagar o contato (Acaciano Never)?</h3>
</div>
<a class="btn btn-danger btn-block" role="button" style="cursor:pointer" asp-controller="Contato" asp-action="Apagar">Confirmar exclusão</a>
<a class="btn btn-primary btn-block" role="button" style="cursor:pointer" asp-controller="Contato" asp-action="Index">Cancelar exclusão</a>
